#last
	padding 100px 0 120px
	background-color: #FFF1D8
	text-align center
	@media screen and (max-width 991px)
		padding 90px 0
		text-align center
	@media screen and (max-width 386px)
		padding 60px 0
.general-img-last-1
	top -440px
	left -300px
	z-index -1
	@media screen and (max-width 991px)
		width 500px
		top -450px
	@media screen and (max-width 560px)
		display none
.general-img-last-2
	bottom -100px
	right -400px
	@media screen and (max-width 991px)
		width 500px
		bottom: 0
		right: -300px
	@media screen and (max-width 560px)
		display none
.last-title
	font-family: L
	font-size: 60px
	@media screen and (max-width 1199px)
		line-height: 1.3
	@media screen and (max-width 991px)
		font-size: 60px
	@media screen and (max-width 560px)
		font-size: 40px
.last-text
	font-size: 22px
	margin 40px 0 50px
	@media screen and (max-width 560px)
		margin 30px 0 40px
.last-img
	position relative
	// padding-top: 100%
	img
		bottom -290px
		right 0
	@media screen and (max-width 991px)
		padding 0
		img
			display block
			margin 50px auto 0
			position: static
			width 100%
			max-width 364px
			@media screen and (max-width: 560px)
				max-width 200px