#main
	padding  0 0 100px
	margin-bottom: 100px
	@media screen and (max-width 991px)
		padding 0 0 60px
		text-align center
		margin-bottom: 60px
	@media screen and (max-width 560px)
		padding: 0 0 40px
		margin-bottom: 40px
.main-date
	display inline-block
	position absolute
	top 0
	left 0
	right 0
	margin 0 auto
	padding 30px 20px 25px
	border-bottom-left-radius 15px
	border-bottom-right-radius 15px
	background-color: #EDCC90
	text-align center
	font-size: 26px
	font-weight: bold
	width 100%
	max-width 335px
	p:last-child
		font-weight: normal
	@media screen and (max-width 991px)
		position: static
		// display table
		width auto
		padding 10px 18px
		border-radius: 15px
		min-width none
		font-size: 20px
		p
			// display table-cell
			// vertical-align middle
			padding-bottom: 15px
			&:last-child
				padding: 15px 0 0
				border-top: 2px solid #fff
.general-bg-main-1
	top -50px
	left -200px
	@media screen and (max-width 560px)
		display none
.general-bg-main-2
	top 0
	right 50px
	@media screen and (max-width 560px)
		display none
.general-bg-main-3
	bottom -100px
	left 300px
	width 600px
	@media screen and (max-width 560px)
		display none
.general-img-main-1
	top: -204px;
	left: -214px;
	width: 500px;
	@media screen and (min-width 1440px)
		width auto
		left: -313px;
		top: -300px;
	@media screen and (max-width 560px)
		display none
.general-img-main-2
	bottom: -200px
	right: -415px
	@media screen and (min-width 1440px)
		bottom: -158px;
		right: -400px;
	@media screen and (max-width 560px)
		display none
.main
	&-pre
		text-align center
		margin-top: 140px
		font-size: 16px
		@media screen and (max-width 991px)
			margin-top: 60px
	&-title
		margin 60px 65px 30px 75px
		font-family: CR
		font-size: 90px
		font-weight: 600
		text-transform uppercase
		color #000
		line-height: 90px
		small
			display block
			font-size: 22px
			text-align center
			line-height: 26px
			@media screen and (max-width 560px)
				font-size: 18px
				line-height: 20px
		@media screen and (min-width 1440px)
			font-size: 109px
			margin 140px 20px 30px 70px
			line-height: 130px
		@media screen and (max-width 1199px)
			font-size: 80px
			margin 100px 0 30px
		@media screen and (max-width 991px)
			margin 50px 0 30px
			font-size: 68px
			line-height: 80px
			span
				display block
				&.main-title-span-1
					margin-left: 20px
					text-align: left
		@media screen and (max-width 767px)
			font-size: 50px
			line-height: 60px
		@media screen and (max-width 560px)
			font-size: 42px
			line-height: 45px
			span
				&.main-title-span-1
					margin-left: 20px
					text-align: center
					margin-left: 0
		@media screen and (max-width 386px)
			font-size: 38px
			line-height: 42px
			margin-top: 38px
			text-align center
			// span.main-title-span-1
			// 	margin-left: 40px
		@media screen and (max-width 320px)
			span.main-title-span-1
				margin-left: 0
				display block
				text-align center
		span.main-title-span-2
			display block
			text-align right
			color #fff
			font-weight: bold
			text-shadow: -1px -1px 0 #4E4C4C, 1px -1px 0 #4E4C4C, -1px 1px 0 #4E4C4C, 1px 1px 0 #4E4C4C, 0 0 0 #4E4C4C, 0 -1px 0 #4E4C4C, -1px 0 0 #4E4C4C, 1px 0 0 #4E4C4C, 0 1px 0 #4E4C4C
			@media screen and (max-width 560px)
				font-size: 42px
				display block
				text-align center
			@media screen and (max-width 386px)
				font-size: 40px
	&-subtitle
		font-size: 30px
		text-align right
		margin-right: 160px
		@media screen and (max-width 991px)
			margin-right: 60px
			font-size: 22px
		@media screen and (max-width 560px)
			font-size: 18px
			margin-right: 35px
			text-align center
			margin-right: 0
	&-author
		font-size: 26px
		font-weight: 700
		color #5BBEAC
		text-align right
		margin 20px 160px 50px
		@media screen and (max-width 991px)
			margin: 20px 60px 50px 0
			font-size: 20px
		@media screen and (max-width 560px)
			font-size: 18px
			margin 15px 0 35px
			text-align center
.button-wrapper-main
	text-align right
	margin-right: 160px
	@media screen and (max-width 991px)
		margin-right: 60px
	@media screen and (max-width 560px)
		margin-right 0
		text-align center