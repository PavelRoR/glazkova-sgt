.main
	&-title
		margin 160px 40px 30px 60px
		font-family: CR
		font-size: 90px
		font-weight: 600
		text-transform uppercase
		color #000
		line-height: 90px
		span
			display block
			text-align right
			color #fff
			font-weight: bold
			text-shadow: -1px -1px 0 #4E4C4C, 1px -1px 0 #4E4C4C, -1px 1px 0 #4E4C4C, 1px 1px 0 #4E4C4C, 0 0 0 #4E4C4C, 0 -1px 0 #4E4C4C, -1px 0 0 #4E4C4C, 1px 0 0 #4E4C4C, 0 1px 0 #4E4C4C
	&-subtitle
		font-size: 30px
		text-align right
		margin-right: 160px
	&-author
		font-size: 26px
		font-weight: 700
		color #5BBEAC
		text-align right
		margin 20px 160px 50px
.form
	width 100%
	max-width 640px
.form-title
	text-align center
	font-size: 26px
	font-weight: bold
	margin-bottom: 10px
.input
	display block
	width 100%
	border: 2px solid #5BBEAC;
	border-radius: 15px;
	font-size: 18px
	color #4E4C4C
	padding 10px 22px
	background-color: #FFF
	margin-bottom: 19px
	&::-webkit-input-placeholder
		color:#4E4C4C
		opacity:1;
	&::-moz-placeholder
		color:#4E4C4C
		opacity:1;
	&:-ms-input-placeholder
		color:#4E4C4C
		opacity:1;
@-webkit-keyframes autofill
	to
		font-size: 18px
		color: #4E4C4C;
		background-color: #FFF
input:-webkit-autofill
	-webkit-animation-name: autofill;
	-webkit-animation-fill-mode: both;
.input:active
input:-internal-autofill-selected
	background-color: #FFF !important
.button-form
	display block
	width 100%
.alert-message
	clear both
	display: none;
	font-size: 16px;
	padding-top: 10px;
	color: #f00
	text-align: center
	font-weight: 700
	line-height: 1
.checking
	display: table;
	width 100%
	// max-width 455px
	margin: 5px auto 0
	text-align: left
@keyframes pulse
	0%
		box-shadow: 0 0 0 0 rgba(red, .12)
	100%
		box-shadow: 0 0 15px 10px rgba(red, .12)
.checking .check-label
	line-height: 33px;
	cursor: pointer;
	position: relative;
	display: table-cell;
	vertical-align: middle;
	padding: 0 0 0 30px;
	text-align: left;
	font-size: 18px
	color #4E4C4C
	@media screen and (max-width 560px)
		line-height: 18px
.checking .check-label a
	text-decoration: none
	border-bottom: 1px dashed transparent
	display: inline
	color #4e4e4e
	&:focus
		border-bottom-color: transparent
		color #4e4e4e
	&:hover
		border-bottom-color: inherit
		text-decoration: none
		color #4e4e4e
.checking .check-label:before
	content: '';
	background-color: red;
	position: absolute;
	left: 0;
	top: 3px;
	width: 25px;
	height: 25px;
	border none
	border-radius: 5px
	animation: pulse
	animation-duration: .7s
	animation-iteration-count: infinite
	animation-timing-function: ease-out;
.checking .check-label:after
	content: '';
	position: absolute;
	left: 9px;
	top: -10px;
	width: 8px;
	height: 14px;
	background-color: transparent;
	opacity: 0;
	border-bottom: 2px solid #fff;
	border-right: 2px solid #fff;
	-webkit-transform: rotate(41deg);
	-ms-transform: rotate(41deg);
	transform: rotate(41deg);
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
.check
	position: absolute;
	z-index: -1;
	opacity: 0;
	width: 0;
	height 0;
	border: 0;
	left -99999px;
.check:checked + .check-label:before
	background-color: #5BBEAC
	animation: none
.check:checked + .check-label:after
	opacity: 1;
	top: 6px;
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;