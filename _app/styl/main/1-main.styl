#main
	padding  0 0 100px
	@media screen and (max-width 991px)
		padding 0 0 60px
		text-align center
.main-date
	display inline-block
	position absolute
	top 0
	left 0
	right 0
	margin 0 auto
	padding 30px 20px 25px
	border-bottom-left-radius 15px
	border-bottom-right-radius 15px
	background-color: #EDCC90
	text-align center
	font-size: 26px
	font-weight: bold
	width 100%
	max-width 335px
	p:last-child
		font-weight: normal
	@media screen and (max-width 991px)
		position: static
		display table
		width auto
		padding 10px 18px
		border-radius: 15px
		min-width none
		font-size: 20px
		p
			display table-cell
			vertical-align middle
			padding-right: 15px
			&:last-child
				padding: 0 0 0 15px
				border-left: 2px solid #fff
.general-bg-main-1
