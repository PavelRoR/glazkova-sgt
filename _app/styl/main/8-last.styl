	background-color: #FFF1D8
	@media screen and (max-width 991px)
		padding 200px 0
	@media screen and (max-width 560px)
		padding 60px 0 120px
	@media screen and (max-width 386px)
		padding 20px 0 90px
.form-last
	margin 0 auto
.general-img-last-1
	top -600px
	left -300px
